{
  "name": "SO-enhanced",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 8.11.3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run assets:prod && npm run copy-js && npm run tsc",
    "start": "node ./dist/server/start.js",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon\"",
    "tsc": "tsc",
    "lint": "tslint \"src/**/*.ts\" ",
    "lint-fix": "tslint --fix src/**/*.ts test/**/*.ts ",
    "copy-js": "mkdir -p dist/server/helpers/sanctuary/ && cp src/server/helpers/sanctuary/*.js dist/server/helpers/sanctuary/",
    "watch": "npm run copy-js && nodemon --exec ./node_modules/.bin/ts-node -- ./dist/server/start.js --ignore public/",
    "dev:start": "concurrently \"npm run watch\" \"npm run assets\" --names \"ðŸ’»,ðŸ“¦\" --prefix name",
    "assets": "webpack -w --mode development --config webpack.config.js",
    "assets:prod": "webpack  --mode production --config webpack.config.js",
    "atdd": "NODE_ENV=test ts-mocha -p ./tsconfig_test.json test/**/*.spec.[tj]s",
    "test": "NODE_ENV=test ts-mocha -p ./tsconfig_test.json src/**/*.spec.[tj]s",
    "test:skip-lint": "NODE_ENV=test ts-mocha -p ./tsconfig_test.json src/**/*.spec.[tj]s",
    "prelint": "tsc",
    "pretest": "npm run lint"
  },
