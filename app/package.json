    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "fluture": "^10.3.1",
    "fluture-sanctuary-types": "^2.2.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.0",
    "sanctuary": "^0.15.1",
    "sanctuary-def": "^0.18.1",
    "uuid": "^3.2.1",
    "validator": "^10.11.0",
    "xss": "^1.0.6"
  },
